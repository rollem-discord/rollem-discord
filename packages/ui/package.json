{
  "name": "@rollem/ui",
  "description": "Rollem (UI) - UI deployable",
  "version": "2.11.0",
  "author": "David <npm@davidsharer.com>",
  "bugs": "https://github.com/rollem-discord/rollem-discord/issues",
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@mui/styles": "^6.1.1",
    "@rollem/common": "workspace:*",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "discord-oauth2": "^2.6.0",
    "dotenv-cli": "^4.0.0",
    "eslint-config-next": "^14.2.13",
    "gray-matter": "^4.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "^14.2.13",
    "next-session": "^4.0.5",
    "pg": "^8.6.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^6.0.3",
    "remark": "^13.0.0",
    "remark-gfm": "^1.0.0",
    "remark-html": "^13.0.1",
    "sass": "^1.79.4",
    "swr": "^2.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/lodash": "^4.14.170",
    "@types/node": "^18",
    "@types/prop-types": "^15.7.13",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^7.0.3",
    "next-cli": "^1.0.0",
    "nodemon": "^3.1.7",
    "npm-watch": "^0.13.0",
    "typescript": "^5.3.3",
    "webpack": "^5.88.2"
  },
  "gitHead": "69a321958280a1817ff7567729d87d6b7617d318",
  "homepage": "https://github.com/rollem-discord/rollem-discord",
  "license": "MIT",
  "main": "rollem.js",
  "packageManager": "yarn@4.5.0",
  "publishConfig": {
    "access": "public"
  },
  "watch": {
    "::ui:build": {
      "patterns": [
        ".",
        "../../*",
        "../../secrets"
      ],
      "extensions": "js,tsx,ts,json,cjs,mjs,env",
      "quiet": false
    }
  },
  "scripts": {
    "____________________note01____________________": "all commands prefixed with `:` need review // TODO: Review",
    "____________________note02____________________": "all non-pinned commands should be prefixed with `::<scope>:` to avoid cross-workspace build actions",
    "____________________note03a____________________": "build = fresh-build",
    "____________________note03b____________________": "watch = fresh-build -> watch",
    "____________________note03c____________________": "dev = fresh-build -> watch -> live-update",
    "____________________note03d____________________": "start = only start",
    "____________________note04e____________________": "package = fresh build + package",
    "__________________________________________________": "end of notes",
    "____________________pinned____________________": "common commands",
    "build": "yarn run ::ui:clean && yarn run ::ui:build",
    "watch": "yarn run ::ui:clean && yarn run ::ui:watch",
    "clean": "yarn run ::ui:clean",
    "dev": "yarn run ::ui:dev",
    "start": "yarn run ::ui:start:dotenv",
    "package": "yarn run ::ui:build && yarn run ::ui:package",
    "package:only": "yarn run ::ui:package",
    "package:start": "yarn run ::ui:package:start",
    "package:cleanup": "yarn run ::ui:package:cleanup",
    "____________________build____________________": "build-related commands",
    "::ui:clean": "rm -rf ./dist ./.next",
    "::ui:dev": "dotenv -e ../../secrets/vscode.env -- next dev",
    "::ui:build": "next build",
    "::ui:watch": "npm-watch ::ui:build",
    "____________________local-run____________________": "commands related to local run",
    "::ui:start": "next start",
    "::ui:start:dotenv": "dotenv -e ../../secrets/vscode.env -- yarn ::ui:start",
    "____________________local-docker-packaging____________________": "commands for running within the docker package",
    "::ui:package:cleanup": "docker rmi $(docker images --filter=label=org.opencontainers.image.ref.name=rollem/ui -aq)",
    "::ui:package": "yarn run ::DF --target rollem-ui --label='rollem-ui' -t rollem-ui",
    "::ui:package:start": "docker run --rm -it --env-file ../../secrets/vscode.env rollem-ui",
    "____________________for-docker____________________": "commands for running within the docker package",
    "____________________unreviewed____________________": "// TODO: Review",
    ":dev:ui": "next dev",
    ":package": "docker build -t rollem-ui .",
    ":package:start": "docker run -p 3001:3000 --env-file ../../secrets/vscode.env rollem-ui",
    ":package:start-remote": "docker run -p 3001:3000 --env-file ../../secrets/vscode.env lemtzas/docker-rollem-ui",
    ":k8s:replace": "kubectl replace -f ../infra/k8s/rollem-ui/",
    ":k8s:update-credentials": "doctl kubernetes cluster kubeconfig save rollem-115-sfo2",
    ":k8s:delete-evicted-pods": "kubectl get pods | grep Evicted | awk '{print $1}' | xargs kubectl delete pod"
  }
}
